{
    plugins: [
      'react'
    ],
    extends: [
      'eslint:recommended',
      'plugin:react/recommended'
    ],
    parser: 'babel-eslint',
    parserOptions: {
      sourceType: 'module',
      ecmaVersion: 6
    },
    env: {
      browser: true,
      node: true,
      jest: true,
      es6: true
    },
    rules: {
      quotes: [2, 'single', { avoidEscape: true }],
      no-console: 0
    },
    settings: {
      react: {
        version: "detect"
      }
    }
  }
  