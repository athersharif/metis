{
  "name": "react-db-google-sheets",
  "version": "2.2.0",
  "description": "React HOC for using Google Sheets as Database",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --ignore src/setupTests.js,src/__tests__ --out-dir dist --no-comments",
    "docs": "jsdoc -c jsdoc.conf.json -d docs/ src/*.js",
    "lint": "eslint src/",
    "lint-staged": "lint-staged",
    "prepublish": "rm -rf dist/* && npm run build",
    "prettier": "prettier --single-quote --write '{,!(coverage|node_modules|docs|dist)/**/}*.{js,css,json}'",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/athersharif/metis.git"
  },
  "keywords": [
    "react",
    "hoc",
    "google",
    "sheets",
    "db",
    "database"
  ],
  "author": "Ather Sharif <ather.sharif@gmail.com>",
  "license": "MIT",
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.{js,json,css},!dist/*.{js,json,css}": [
      "npm run prettier",
      "git add -u"
    ]
  },
  "jest": {
    "automock": false,
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdoc": "^4.0.2",
    "lint-staged": "^13.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
